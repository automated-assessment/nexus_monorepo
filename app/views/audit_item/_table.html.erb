<table class="table table-hover">
  <thead>
    <tr>
      <th width="15%">Date</th>
      <th width="10%">Level</th>
      <th>Info</th>
    </tr>
  </thead>
  <tbody>
    <% items.each do |a| %>
      <tr>
        <th scope="row"><%= pretty_datetime a.timestamp %></th>
        <td><%= a.level.capitalize %></td>
        <td><%= a.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
